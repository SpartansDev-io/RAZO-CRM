{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:debug": "cross-env DEBUG=eslint:* eslint src --ext .ts,.tsx",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "read -p 'Nombre de la migraciÃ³n: ' name && npx prisma migrate dev --name $name",
    "prisma:migrate:reset": "npx prisma migrate reset dev --name init",
    "prisma:seed": "npx prisma db seed",
    "prepare": "husky",
    "pre-push": "npm run format  && npm run lint && npm run test && npm run build",
    "pre-commit": "npx lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
